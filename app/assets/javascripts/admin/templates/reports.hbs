<div class="row pull-right mar_tm33">

    <div class='view-options' style="padding-right: 15px;">
        {{#if viewingTable}}
            {{i18n 'admin.dashboard.reports.view_table'}}
        {{else}}
            <a href {{action "viewAsTable"}}>{{i18n 'admin.dashboard.reports.view_table'}}</a>
        {{/if}}
        |
        {{#if viewingGraph}}
            {{i18n 'admin.dashboard.reports.view_graph'}}
        {{else}}
            <a href {{action "viewAsGraph"}}>{{i18n 'admin.dashboard.reports.view_graph'}}</a>
        {{/if}}
    </div>
</div>

{{#if viewingGraph}}
    {{admin-graph model=model rowvalues=rowvalues}}
{{else}}
    {{component reportsType model=model refreshReportAction="refreshReport" rowvalues=rowvalues}}
{{/if}}
