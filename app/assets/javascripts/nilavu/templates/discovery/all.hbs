<div class="rt-box col-xs-12 c_bg_white c_height_5">
    {{topic-list-header currentUser=currentUser title=dashboardTitle viewIcons="true" gridView=isGrid listView=isList}}
    <div class="bottom-divider"></div>
    <div {{bind-attr class="isGrid:gridViewVisible:gridViewHidden"}} id="app-grid-view">
        <div class='contents'>
            <div class="row c_pading-b15">
                <div class="portlet-body-head">
                    <div class="col-xs-9 col-sm-9 col-md-9 col-lg-9 col-xl-9 cat_title">
                        <span class="darker-black">
                          <i class="topics_header_torpedo"></i>
                          <span class="title">< {{i18n 'dashboards.torpedo.title'}}</span>
                        </span>
                        <span style="display: block; margin-top: 10px;">
                          <span style="color: #6fde86">
                            {{topic-count inkTopicsModel "torpedo"}}
                          </span>
                          {{i18n 'dashboards.torpedo.subtitle'}}
                        </span>
                    </div>
                </div>
            </div>
            <div class="bottom-divider"></div>
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                <div class="row app_box app_storage">
                    <a href {{action "createTorpedo"}}>
                        <div class="col-xs-6 col-sm-5 col-md-4 col-lg-3 col-xl-2 app_cover">
                            <div class="app_inner">
                                <div class="app_new {{if isLoadingTorpedo "no-pad" "padding"}}">
                                    <span class="glyphicon {{if isLoadingTorpedo "spin extra-large" "glyphicon-plus"}}"></span>
                                    <p>
                                        {{#unless isLoadingTorpedo}}
                                            {{i18n 'dashboards.torpedo.create'}}
                                        {{/unless}}
                                    </p>
                                </div>
                            </div>
                        </div>
                    </a>
                    {{#if hasTopics}}
                        {{topic-grid showCategory=torpedoCategory visibleShowMore=true showTopicPostBadges=showTopicPostBadges showPosters=true currentUser=currentUser canBulkSelect=canBulkSelect changeSort="changeSort" toggleBulkSelect="toggleBulkSelect" hideCategory=model.hideCategory
                        order=order ascending=ascending bulkSelectEnabled=bulkSelectEnabled selected=selected expandGloballyPinned=expandGloballyPinned expandAllPinned=false topics=inkTopicsModel}}
                    {{/if}}
                </div>
            </div>
        </div>
    </div>
    <div {{bind-attr class="isGrid:gridViewVisible:gridViewHidden"}} id="app-grid-view">
        <div class='contents'>
            <div class="row c_pading-b15">
