<div class="row">
    <div class="col-md-6 info-center">
        <table>
            <tr>
                <td>
                    <h5><b>{{content_cpu_cores}}</b></h5></td>
                <td>{{cpu_cores}}</td>
            </tr>
        </table>
    </div>
</div>
<div class="ssh-well-without-background">
    <div class="row">
     {{#if disableMonitor}}
         <div class="col-md-12 info center disable-banner">
             <span>{{i18n "vm_management.network.monitoring_cpu"}} {{osName}}.</span>
         </div>
     {{else}}
         <div class="col-md-12 info">
             {{chart-cpu model=model selectedTab=selectedTab}}
         </div>

     {{/if}}
    </div>
</div>
