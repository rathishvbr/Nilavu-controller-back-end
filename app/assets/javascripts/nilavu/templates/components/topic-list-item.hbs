    <td class="td-left">{{topic.name}}</td>
    <td class="set_title"> {{topic-link topic}} </td>
    <td> {{showBrandImage}} </td>
    <td><i class="mt12-5 circle-{{topic.state}}"></i> <span class="mar_l10">{{topic.state}}</span></td>
    <td>
        <ul class="list-inline app_icon_bottom">
            <li class="list-inline-item mt12-5">
                <!-- <a href="" class="c_icon-window pull-right"></a> -->
                {{#if shell}}
                  {{#link-to "shell" (query-params id=topic.id asms_id=topic.asms_id name=name) target="_blank" }}
                  <i class="c_icon-window pull-right" style="padding-top:20px"></i>
                  {{/link-to}}
                {{else}}
                  {{#link-to "vnc" (query-params id=topic.id asms_id=topic.asms_id name=name type=modelType ) target="_blank" }}
                  <i class="c_icon-window pull-right" style="padding-top:20px"></i>
                  {{/link-to}}
                {{/if}}
            </li>
        </ul>
    </td>
